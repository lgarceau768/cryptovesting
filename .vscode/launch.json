  "version": "0.2.0",
  "configurations": [
  {
    "name": "Attach by Process ID",
    "processId": "${command:PickProcess}",
    "request": "attach",
    "skipFiles": [
      "<node_internals>/**"
    ],
    "type": "pwa-node"
  },
    {
      "name": "Buy BSUD Python",
      "type": "python",
      "request": "launch",
      "program": "app\\worker_manager\\workers\\buyWorker.py",
      "args": [
        "-t", "0xed24fc36d5ee211ea25a80239fb8c4cfd80f12ee", 
        "-u", "test", 
        "-a", "0.05", 
        "-s", "0.8"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Buy FEC Python",
      "type": "python",
      "request": "launch",
      "program": "app\\worker_manager\\workers\\buyWorker.py",
      "args": [
        "-t", "0x4108BbbC90B991E41B7DeE69Bd91c8ad97e26CBF", 
        "-u", 
        "test", 
        "-a", "0.05", 
        "-s", "0.8"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Sell BUSD Python",
      "type": "python",
      "request": "launch",
      "program": "app\\worker_manager\\workers\\sellWorker.py",
      "args": [
        "-t", "0xed24fc36d5ee211ea25a80239fb8c4cfd80f12ee", 
        "-u", "test", 
        "-a", "0.1", 
        "-s", "0.8"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Sell FEC Python",
      "type": "python",
      "request": "launch",
      "program": "app\\worker_manager\\workers\\sellWorker.py",
      "args": [
        "-t", "0x4108BbbC90B991E41B7DeE69Bd91c8ad97e26CBF", 
        "-u", "test", 
        "-a", "5000", 
        "-s", "0.8"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Watch FEC",
      "type": "python",
      "request": "launch",
      "program": "app\\worker_manager\\workers\\tokenWatcherWorker.py",
      "args": [
        "-t", "0x4108BbbC90B991E41B7DeE69Bd91c8ad97e26CBF", 
        "-u", "test", 
        "-a", "48425002790017424", 
        "-i", "60688557149", 
        "-p", "1.5"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Pull Contract",
      "type": "python",
      "request": "launch",
      "program": "Z:\\Repos\\cryptovesting\\scripts\\rugmenot_contracts\\pull_contract.py",
      "args": ["0x5afef8567414f29f0f927a0f2787b188624c10e2"],
      "console": "integratedTerminal"
    },
    {
      "name": "Contract Check",
      "type": "python",
      "request": "launch",
      "program": "Z:\\Repos\\cryptovesting\\scripts\\rugmenot_contracts\\contract_check.py",
      "args": ["safematic.sol"],
      "console": "integratedTerminal"
    },
    {
      "name": "Swap Token",
      "program": "${workspaceFolder}/sniper_bot/js/swap.js",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "args": ["0xf1e35511e8a599bca9385f6e0a02bdd203290bda"],
      "type": "pwa-node"
    },
    {
      "name": "Liquidity Watcher",
      "type": "python",
      "request": "launch",
      "program": "Z:\\Repos\\cryptovesting\\sniper_bot\\py\\liquidity_watcher.py",
      "args": ["-t", "0xf1e35511e8a599bca9385f6e0a02bdd203290bda"],
      "console": "integratedTerminal"
    },
    {
      "name": "Liquidity Pool Check",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "args": ["-s", "lp"],
      "console": "integratedTerminal"
    },
    {
      "name": "Sniff Existing Coins",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "args": ["-s", "sc"],
      "console": "integratedTerminal"
    },
    {
      "name": "Scrape Fomo and Sniffer",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "args": ["-s", "sfrs"],
      "console": "integratedTerminal"
    }
  ]
}
